<?xml version="1.0" encoding="utf-8"?>
<LinearLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="1">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/searchresultimage"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/history_svgrepo_com"
        android:tint="@color/charcoal_black"
        android:background="@drawable/circleyellowfil"
        android:backgroundTint="@color/new_gray"
        android:scaleType="centerInside"
        android:layout_marginStart="15dp"
        android:layout_gravity="center"
        />
    <TextView
        android:id="@+id/searchresulttext"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:layout_weight="0.9"
        android:text="Seach result"
        android:textSize="15dp"
        android:textColor="@color/charcoal_black"
        android:fontFamily="@font/interregular"
        android:layout_gravity="center"
        android:layout_marginStart="20dp"
        />
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/arrow_up_left_svgrepo_com"
        android:tint="@color/charcoal_black"
        android:layout_margin="10dp"
        android:scaleType="centerInside"
        android:layout_gravity="center"
        android:layout_weight="0.1"
        />

</LinearLayout>